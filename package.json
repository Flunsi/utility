{
	"name": "@flunsi/utility",
	"version": "1.0.0",
	"description": "Web utility functions for my projects.",
	"keywords": [
		"javascript",
		"typescript",
		"utility",
		"web",
		"string",
		"number",
		"array"
	],
	"author": "Niklaus Flunser",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Flunsi/utility.git"
	},
	"bugs": {
		"url": "https://github.com/Flunsi/utility/issues"
	},
	"type": "module",
	"files": [
		"dist/*"
	],
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"lint": "eslint",
		"test": "vitest run",
		"build": "tsup",
		"zip": "npm pack --pack-destination=./package",
		"all": "npm run lint && npm run test && npm run build && npm run zip",
		"release": "npm publish --access public"
	},
	"devDependencies": {
		"typescript": "^5.6.3",
		"tsup": "^8.3.5",
		"@eslint/js": "^9.14.0",
		"eslint": "^9.14.0",
		"globals": "^15.12.0",
		"typescript-eslint": "^8.14.0",
		"vitest": "^2.1.5"
	}
}
